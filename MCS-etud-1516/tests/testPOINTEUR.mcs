int * malloc(int taille){
  int adr;
  // appel a l'allocation TAM
  asm {
    LOAD %taille    ; acces au parametre taille
    SUBR Malloc     ; allocation
    STORE %adr      ; resultat dans adr
  }
  return adr;
}

